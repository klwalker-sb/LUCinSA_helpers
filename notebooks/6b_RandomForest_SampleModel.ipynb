    "#### model_name = pixdf_ratios\n",
    "##   first collect the sample ratios from the LUT\n",
    "\n",
    "def new_training_data(bal_ratio, new_name, cutoff = 400):\n",
    "    \n",
    "    lut=pd.read_csv('../Class_LUT.csv')\n",
    "    lut.drop(['Description'], axis=1, inplace=True)\n",
    "    #print(lut.sort_values('perLC17E')[\"perLC17E\"])\n",
    "    ratios = lut.sort_values('perLC17E')[\"perLC17E\"]\n",
    "    names = lut.sort_values('perLC17E')[\"LC25_name\"]\n",
    "    ordered = pd.concat([ratios, names], axis = 1)\n",
    "    #print(ordered)\n",
    "    \n",
    "    ##   now clean this df for NaN and repeats, check if the percents add up correctly, then divide and round\n",
    "    \n",
    "    ordered = ordered.dropna()\n",
    "    ordered = ordered.drop_duplicates(subset = \"perLC17E\") \n",
    "    #print(ordered)\n",
    "    \n",
    "    tot = 0\n",
    "    for i in ordered[\"perLC17E\"]:\n",
    "        tot += i\n",
    "    \n",
    "    # print(tot)\n",
    "    # lessssss gooooooo close to 1\n",
    "    \n",
    "    min = ordered[\"perLC17E\"][33]\n",
    "    ordered[\"perLC17E\"] = round(ordered[\"perLC17E\"]/min)\n",
    "    \n",
    "    # print(ordered)\n",
    "    \n",
    "    ##   find the scale factor, actually just use an appropriate number\n",
    "    \n",
    "    ## rescale the training data set\n",
    "    \n",
    "    counts = pixdf['LC25_name'].value_counts().rename_axis(\"LC25_name\").reset_index(name=\"counts\")\n",
    "    #print(sum(counts[\"counts\"]))\n",
    "    \n",
    "    ratios = []\n",
    "    num1 = 0\n",
    "    for i in ordered[\"LC25_name\"]:\n",
    "        num2 = 0\n",
    "        for j in counts[\"LC25_name\"]:\n",
    "            if i == j:\n",
    "                if counts.iloc[num2][1] >= cutoff:\n",
    "                    \"\"\"\n",
    "                    print(ordered.iloc[num1][1])\n",
    "                    print(ordered.iloc[num1][0] * 7)\n",
    "                    print(counts.iloc[num2][1])\n",
    "                    \"\"\"\n",
    "                    ratios.append((ordered.iloc[num1][0] * bal_ratio)/counts.iloc[num2][1])\n",
    "                elif counts.iloc[num2][1] < cutoff:\n",
    "                    ratios.append(1)\n",
    "            num2 += 1\n",
    "        num1 += 1\n",
    "    \n",
    "    #print(ratios)\n",
    "    \n",
    "    num3 = 0\n",
    "    pixdf_ratios_rebal = pixdf\n",
    "    for i in ratios:\n",
    "        name = ordered.iloc[num3][1]\n",
    "        #print(name)\n",
    "        pixdf_ratios_rebal = pixdf_ratios_rebal[(pixdf_ratios_rebal['rand'] < i) | (pixdf_ratios_rebal[\"LC25_name\"] != name)]\n",
    "        #print(pixdf_ratios_rebal['LC25_name'].value_counts())\n",
    "        num3 += 1\n",
    "\n",
    "    #print('pixdf_ratios_rebal sample breakdown by LC25 class:')\n",
    "    #print(pixdf_ratios_rebal['LC25_name'].value_counts())\n",
    "    pixdf_path = os.path.join(classification_params['local_dir'],'r_tests/pixdf_{}_rebal_{}.csv'.format(new_feature_mod, new_name))\n",
    "    pd.DataFrame.to_csv(pixdf_ratios_rebal, pixdf_path)\n",
    "    \n",
    "    print(len(pixdf_ratios_rebal[\"LC25_name\"]))\n",
    "\n",
    "# use this to fix any mess ups u make \n",
    "\"\"\"\n",
    "dir = \"/home/downspout-cel/paraguay_lc/classification/RF\"\n",
    "for filename in os.listdir(dir):\n",
    "    if filename.startswith(\"pixdf_{}\".format(new_feature_mod)):\n",
    "        filepath = os.path.join(dir, filename)\n",
    "        os.remove(filepath)\n",
    "\"\"\"\n",
    "\n",
    "for i in range(1, 20):\n",
    "    bal = math.exp(i / 7)\n",
    "    #print(bal)\n",
    "    new_training_data(bal_ratio = bal, new_name = \"{}\".format(i), cutoff = 400)\n",
    "\n",
    "new_training_data(bal_ratio = 5.5, new_name = \"base\", cutoff = 400)"
